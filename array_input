#include<iostream>
using namespace std;

int main(){
int size;

cout<<"Enter the size of the array:<<endl;
cin>>size;

int a[size];

for(int i = 0;i<size;i++){
  cout<<"Enter element"<<i+1;
  cin>>a[i];
  }

cout<<"Elements of the array are:"<<endl;
for(int i = 0;i<size;i++){
  cout<<"Enter element"<<i+1;
  cout<<a[i];
  }

return 0;
}
